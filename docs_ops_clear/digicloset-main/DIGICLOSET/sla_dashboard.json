{
  "dashboard": {
    "title": "SLA Overview",
    "panels": [
      {
        "type": "graph",
        "title": "Uptime (24h)",
        "targets": [
          {
            "expr": "avg_over_time(probe_success[24h])*100"
          }
        ]
      },
      {
        "type": "graph",
        "title": "P95 API Latency (s)",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))"
          }
        ]
      },
      {
        "type": "graph",
        "title": "Error Rate (%)",
        "targets": [
          {
            "expr": "rate(http_requests_total{status=~\"5..\"}[5m])*100"
          }
        ]
      }
    ],
    "schemaVersion": 30,
    "version": 1
  }
}